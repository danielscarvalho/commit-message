number1=$[ $RANDOM % 10000 + 1]
number2=$[ $RANDOM % 10000 + 1]
number3=$[ $RANDOM % 1164 + 1]
pa1=$(sed $number1!d wordlist.10000.txt)
pa2=$(sed $number2!d wordlist.10000.txt)
pa3=${emoji:$number3:1}
echo "git commit -m \"$pa1 $pa2 $pa3\""

